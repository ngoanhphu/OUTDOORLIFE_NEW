    <!-- Comment Script -->
    <script>
        function addComment() {
            // L?y giá tr? c?a ô nh?p li?u
            var commentInput = document.getElementById('commentInput');
            var commentText = commentInput.value.trim();

            // Ki?m tra n?u ô nh?p li?u không tr?ng
            if (commentText !== "") {
                // T?o ph?n t? div ch?a bình lu?n m?i
                var newComment = document.createElement('div');
                newComment.classList.add('comment-item', 'border', 'p-3', 'mb-3');
                newComment.textContent = commentText;

                // Thêm bình lu?n m?i vào danh sách bình lu?n
                var commentList = document.getElementById('commentList');
                commentList.appendChild(newComment);

                // Xóa n?i dung trong ô nh?p li?u sau khi ??ng bình lu?n
                commentInput.value = '';
            }
        }
    </script>

